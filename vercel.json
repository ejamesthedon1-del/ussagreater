{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/login-data",
      "dest": "/api/login-data.js"
    },
    {
      "src": "/admin-flow-control-secret-2024",
      "dest": "/api/admin-page.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/logon",
      "dest": "/logon.html"
    }
  ]
}
